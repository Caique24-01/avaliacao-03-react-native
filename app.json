{
  "expo": {
    "name": "avaliacao-03-react-native",
    "slug": "avaliacao-03-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rocket.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#292b2c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rocket.png",
        "backgroundColor": "#292b2c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
